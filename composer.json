{
    "name": "php-ws-rest",
    "description": "simple REST service",
    "version": "0.0.3-dev",
    "type": "library",
    "config": {
	   "secure-http": false
    },
    "source": {
	   "type": "git",
	   "url": "git://data.shanknet.local:/usr/local/git/composer/web/php-ws-rest/",
	   "reference": "0.0.3-dev"
    },
    "repositories": [
    	{
    	    "type": "composer",
    	    "url": "http://web.shanknet.local/composer/repo"
    	}
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "require": {
	   "php-basic-patterns": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "web\\ws\\rest\\exception\\": "src/exception/",
            "web\\ws\\rest\\resolver\\": "src/resolver/",
            "web\\ws\\rest\\serve\\": "src/serve/",
            "web\\ws\\rest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "web\\ws\\rest\\test\\": "tests/"
        }
    },
    "keywords": [
    	"REST",
    	"webservice"
    ],
    "license": "GPL-3.0+",
    "authors": [
    	{
    	    "name": "Claus Petersen",
    	    "role": "Developer"
    	}
    ]
}
